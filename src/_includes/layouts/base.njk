<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>{{ metadata.title or eleventyNavigation.key }}</title>
    <meta name="description" content="{{ description or metadata.description }}">
    <link rel="stylesheet" href="{{ '/css/index.css' | url }}">
    <link rel="stylesheet" href="{{ '/css/prism-base16-monokai.dark.css' | url }}">
    <link rel="stylesheet" href="/css/{{ cssFile }}"/>
    <link rel="alternate" href="{{ metadata.feed.path | url }}" type="application/atom+xml" title="{{ metadata.title }}">
    <link rel="alternate" href="{{ metadata.jsonfeed.path | url }}" type="application/json" title="{{ metadata.title }}">
    <link rel="icon" href="../../img/favicon.ico">
    <script defer src="https://analytics.eu.umami.is/script.js" data-website-id="0a19d71c-401e-4c3d-bf2a-9ec4ad239fa6"></script>
  </head>
  <body>
    <header>
      <!--
      <h1 class="home"><a href="{{ '/' | url }}"></a></h1>

      {#- Read more about `eleventy-navigation` at https://www.11ty.dev/docs/plugins/navigation/ #}
      <ul class="nav">
      {%- for entry in collections.all | eleventyNavigation %}
        {#{% if entry.url == page.url %} nav-item-active{% endif %}#}
        <li class="nav-item"><a href="{{ entry.url | url }}">{{ entry.title }}</a></li>
      {%- endfor %}
      </ul>
      -->
    </header>

    <main{% if templateClass %} class="{{ templateClass }}"{% endif %}>

      <h1 class="about"><a href="{{ '/' | url }}">Kien Nguyen</a></h1>

      {{ content | safe }}
    </main>

    <footer></footer>

    <!-- Current page: {{ page.url | url }} -->
  </body>
</html>
